<?xml version="1.0" encoding="utf-8"?>
<schemalist>
  <schema id="org.wayfire.plugin.switcher" path="/org/wayfire/plugin/switcher/">
    <key name="duration" type="i">
      <default>250</default>
      <summary>Duration</summary>
    </key>
    <key name="initial-animation" type="i">
      <default>150</default>
      <summary>Initial Animation</summary>
    </key>
    <key name="view-thumbnail-size" type="d">
      <default>0.4</default>
      <summary>Thumbnail Size</summary>
    </key>
    <key name="activate" type="s">
      <default>'&lt;alt&gt; KEY_TAB'</default>
      <summary>Activate</summary>
    </key>
    <key name="next" type="s">
      <default>'KEY_RIGHT'</default>
      <summary>Next</summary>
    </key>
    <key name="prev" type="s">
      <default>'KEY_LEFT'</default>
      <summary>Previous</summary>
    </key>
    <key name="exit" type="s">
      <default>'KEY_ENTER'</default>
      <summary>Exit</summary>
    </key>
  </schema>
</schemalist>
